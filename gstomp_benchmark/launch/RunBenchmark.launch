<launch>
	<arg name="robot" default="$(optenv ROBOT !!NO_ROBOT_SET!!)"/>
	<arg name="pipeline" default="ompl" />
	<arg name="load_robot_description" default="false"/>
	<arg name="debug" default="false" />
		
	<rosparam command="load" file="$(find ipa_moveit_config)/robots/$(arg robot)/moveit/config/$(arg pipeline)_config.yaml"/>
 		
	<!-- move group -->
	<!--include file="$(find ipa_moveit_bringup)/launch/move_group.launch">
		<arg name="robot" value="$(arg robot)"/>
		<arg name="pipeline" value="$(arg pipeline)"/>
		<arg name="allow_trajectory_execution" value="true"/>
	      	<arg name="fake_execution" value="true"/>
	      	<arg name="debug" value="$(arg debug)"/>
	</include>
	
	<include file="$(find ipa_moveit_bringup)/launch/rviz.launch">
    	<arg name="robot" value="$(arg robot)" />
    	<arg name="config" value="true"/>
    	<arg name="debug" value="$(arg debug)"/>
  	</include-->

	<!-- Launch benchmark node -->
	<node name="gstomp_benchmark" pkg="gstomp_benchmark" type="gstomp_run_benchmark" output="screen" required="true"/>
  
</launch>
